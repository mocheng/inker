GEMINI_API_KEY=your_api_key_here
GEMINI_MODEL=gemini-2.0-flash
USE_MOCK_MODEL=false

# OpenTelemetry OTLP Endpoints
# Default: Jaeger at http://localhost:4318

# Base endpoint (path will be appended: /v1/traces, /v1/logs)
# OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318

# Or set specific endpoints:
# OTEL_EXPORTER_OTLP_TRACES_ENDPOINT=http://localhost:4318/v1/traces
# OTEL_EXPORTER_OTLP_LOGS_ENDPOINT=http://localhost:4318/v1/logs

# For Genkit (traces only, no logs support):
# OTEL_EXPORTER_OTLP_TRACES_ENDPOINT=http://localhost:4033/api/otlp
